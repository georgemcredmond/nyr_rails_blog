<% provide(:title, "Log in") %>
<h2 id="style_h2">Log in</h2>

<h3><div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :username %>:
      <%= f.text_field :username%>
<br>
<br>
      <%= f.label :password %>:
      <%= f.password_field :password%>
<br>
<br>
      <%= f.submit "Log in"%>
    <% end %>
    <h3>
  </div>
</div>
